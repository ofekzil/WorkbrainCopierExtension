{
    "manifest_version" : 3,
    "name" : "Workbrain Calendar Copier",
    "version" : "1.0",
    "description" : "copies workbrain schedule to personal calendar",
    "permissions" : ["scripting", "tabs", "activeTab"],
    "host_permissions" : ["https://*.workbrain.cineplex.com/*"],
    "icons" : {
        "16" : "./images/logo.png",
        "48" : "./images/logo.png",
        "128" : "./images/logo.png"
    },

    "action" : {
        "default_popup" : "popup.html",
    
        "default_icon" : {
            "16" : "./images/logo.png",
            "48" : "./images/logo.png",
            "128" : "./images/logo.png"
        }
    },

    "background" : {
        "service_worker" : "saveCalendar.js"
    }
    
    
}