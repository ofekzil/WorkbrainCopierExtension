{
    "manifest_version" : 3,
    "name" : "Workbrain Calendar Copier",
    "version" : "1.0",
    "description" : "downloads workbrain calendar for future shifts as a .json file",
    "permissions" : ["scripting", "tabs"],
    "host_permissions" : ["https://*.workbrain.cineplex.com/*"],
    "icons" : {
        "16" : "./images/logo.png",
        "48" : "./images/logo.png",
        "128" : "./images/logo.png"
    },

    "action" : {
        "default_popup" : "./extension/popup.html",
    
        "default_icon" : {
            "16" : "./images/logo.png",
            "48" : "./images/logo.png",
            "128" : "./images/logo.png"
        }
    }

    

    
    
    
}